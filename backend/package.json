{
  "name": "nightscout-modern-backend",
  "version": "1.0.0",
  "description": "Backend moderno para Nightscout com Node.js, Express e MongoDB",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "nightscout",
    "cgm",
    "diabetes",
    "glucose",
    "monitoring"
  ],
  "author": "Diego Castilho",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.1.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.4",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.19.11",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.39.3",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3"
  }
}
