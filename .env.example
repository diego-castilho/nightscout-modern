# ============================================================================
# Nightscout Modern - Environment Variables
# ============================================================================
# Copy this file to .env and configure with your values
# ============================================================================

# Timezone
TZ=America/Sao_Paulo

# MongoDB Credentials (from existing Nightscout)
MONGODB_USER=
MONGODB_PASSWORD=

# Nightscout API Secret (from existing Nightscout)
NIGHTSCOUT_API_SECRET=your-nightscout-api-secret-here

# Nightscout Modern JWT Secret (generate a new random string)
NIGHTSCOUT_MODERN_JWT_SECRET=your-jwt-secret-here-generate-random-string

# LibreLink Credentials (optional - for MCP integration)
LIBRELINK_USERNAME=
LIBRELINK_PASSWORD=
